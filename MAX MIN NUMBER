.MODEL SMALL
.STACK 100H
.DATA
    MSG1 DB "ENTER FIRST : $"
    MSG2 DB "ENTER SECOND : $"
    MAX_MSG DB "MAXIMUM: $"
    MIN_MSG DB "MINIMUM: $"
    MAX_RESULT DB " $"
    MIN_RESULT DB " $"
.CODE
  MAIN PROC
    MOV AX, @DATA
    MOV DS, AX
    ;output
    MOV AH, 9
    LEA DX, MSG1
    INT 21H
    ;input
    MOV AH, 1
    INT 21H
    MOV BL, AL  
    ;output
    MOV AH, 9
    LEA DX, MSG2
    INT 21H 
    ;input
    MOV AH, 1
    INT 21H
    MOV BH, AL 
    
    ;cmp
    CMP BL, BH
    JE NUMBERS_EQUAL
    JLE SECOND_IS_MAX
    JMP FIRST_IS_MAX
FIRST_IS_MAX:
    MOV MAX_RESULT, BL
    MOV MIN_RESULT, BH
    JMP DISPLAY_RESULTS
SECOND_IS_MAX:
    MOV MAX_RESULT, BH
    MOV MIN_RESULT, BL
    JMP DISPLAY_RESULTS
NUMBERS_EQUAL:
    MOV MAX_RESULT, BL
    MOV MIN_RESULT, BL
DISPLAY_RESULTS:
    MOV AH, 9
    LEA DX, MAX_MSG
    INT 21H
    MOV DL, MAX_RESULT
    MOV AH, 2
    INT 21H 
    
    
    MOV AH, 9
    LEA DX, MIN_MSG
    INT 21H
    MOV DL, MIN_RESULT
    MOV AH, 2
    INT 21H 
    
    
    MOV AH, 4CH
    INT 21H
  MAIN ENDP
END MAIN
