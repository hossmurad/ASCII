.MODEL SMALL
.STACK 100H
.DATA
 NEW_LINE DB 13,10, "$"
 MSG1 DB "HELLOW BANGLADESH $"
 MSG2 DB "bOYCOT INDIA $"
.CODE
 MAIN PROC
     MOV AX,@DATA
     MOV DS,AX
    
     MOV AH,9
     LEA DX,MSG1 
     INT 21H 
     
     MOV AH,9
     LEA DX,NEW_LINE
     INT 21H
     
      
     MOV AH,9
     LEA DX,MSG2
     INT 21H
    
    
    MOV AH,4CH
    INT 21H
    MAIN ENDP
 END MAIN
